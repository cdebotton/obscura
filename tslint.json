{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-react"
  ],
  "rules": {
    "interface-over-type-literal": false,
    "no-submodule-imports": false,
    "no-unused-expression": [true, "allow-tagged-template"],
    "variable-name": [true, "allow-pascal-case"]
  }
}
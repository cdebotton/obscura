{
  "presets": [
    "stage-0",
    "react",
    [
      "env",
      {
        "targets": {
          "node": "current"
        },
        "loose": true
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "styled-components",
    [
      "module-resolver",
      {
        "root": ["./.tmp/ts"],
        "alias": {
          "@client": "client",
          "@components": "components",
          "@containers": "containers",
          "@http": "http",
          "@routes": "routes"
        }
      }
    ]
  ]
}
